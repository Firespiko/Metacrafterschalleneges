pragma solidity 0.8.18;

contract Four_Functions{

    function Sumnum(uint _x,uint _y) external pure returns(uint){
        return _x + _y;
    }

    function Subnum(uint _x,uint _y) external pure returns(uint){
        return _x - _y;
    }

    function mulnum(uint _x,uint _y) external pure returns(uint){
        return _x * _y;
    }

    function Divnum(uint _x,uint _y) external pure returns(uint){
        return _x / _y;
    }
    
}
